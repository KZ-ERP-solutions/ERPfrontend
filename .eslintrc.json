{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "max-len": 0,
    "no-nested-ternary": 0,
    "react/jsx-props-no-spreading": 0,
    "camelcase": 0,
    "no-console": 0,
    "no-restricted-syntax": 0
  }
}
